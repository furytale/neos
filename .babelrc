{
  "presets": [
    ["env", {
      "modules": false,
      "useBuiltIns": true,
      "exclude": [
        "es7.string.pad-end",
        "es7.string.pad-start"
      ],
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "Android >= 4",
          "Explorer >= 10",
          "Firefox >= 40",
          "iOS >= 7",
          "Safari >= 7"
        ]
      }
    }
  ],
  "react",
  "stage-0"
  ],

  "plugins": [
    "transform-react-inline-elements",
    "transform-decorators-legacy",
    "transform-object-assign"
  ],

  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements"
      ]
    }
  }
}